@{
    ViewData["Title"] = "Informator Zborowy";
}
@model HtmlFileData
@section HeaderScripts{
    <style>
        .isDisabled {
            color: currentColor;
            cursor: not-allowed;
            opacity: 0.5;
            text-decoration: none;
        }
    </style>
}
<div class="max-width-1000" style="padding-top: 20px;">
    @await Html.PartialAsync("~/Views/Shared/_Baner.cshtml")
    <h1><i class="fas fa-circle-info"></i> @ViewData["Title"]</h1><br />
    
    @if (Model != null)
    {
        @Html.Raw(Model.Data)
    }
</div>
